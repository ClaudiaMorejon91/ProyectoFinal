<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ProyectoFinal.Login">
    <ContentPage.Content>
        <StackLayout>

            <Label Text="Inicio de sesión"/>
            <Label Text="PROBANDO LOS CAMBIOS EN GITHUB ClaudiaMorejon91" FontSize="Large"/>
            <Entry x:Name="txtUsuario" 
                   Placeholder="Ingrese el usuario "/>
            <Entry x:Name="txtClave" 
                   Placeholder="Ingrese la clave"
                   IsPassword="True"/>

            <Button x:Name="btnIngresar" Text="Ingresar" Clicked="btnIngresar_Clicked"/>

            <Button x:Name="btnRegistrar" Text="Registrarse" Clicked="btnRegistrar_Clicked"/>



        </StackLayout>
    </ContentPage.Content>
</ContentPage>